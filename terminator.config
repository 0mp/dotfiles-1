[global_config]
  borderless = True
  handle_size = 1
[keybindings]
  split_vert = None
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_darkness = 0.08
    cursor_color = "#cccccc"
    font = xos4 Terminus Bold 14
    foreground_color = "#fff7cb"
    login_shell = True
    palette = "#282828:#cc241d:#98971a:#d79921:#458588:#b16286:#689d6a:#a89984:#928374:#fb4934:#b8bb26:#fabd2f:#83a598:#d3869b:#8ec07c:#ebdbb2"
    scrollback_lines = 5000
    scrollbar_position = hidden
    show_titlebar = False
    urgent_bell = True
    use_system_font = False
  [[dark]]
    background_color = "#ffffff"
    cursor_color = "#000000"
    foreground_color = "#000000"
  [[light]]
    background_color = "#d0d0d0"
    background_darkness = 0.78
    background_type = transparent
    cursor_color = "#000000"
    font = Inconsolata Bold 12
    foreground_color = "#000000"
    show_titlebar = False
    use_system_font = False

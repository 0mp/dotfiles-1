[global_config]
  borderless = True
  handle_size = 1
[keybindings]
  split_vert = None
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_darkness = 0.08
    cursor_color = "#cccccc"
    font = Hack 16
    foreground_color = "#cccccc"
    login_shell = True
    scrollback_lines = 5000
    scrollbar_position = hidden
    show_titlebar = False
    urgent_bell = True
    use_system_font = False
  [[dark]]
    background_color = "#ffffff"
    foreground_color = "#000000"
  [[light]]
    background_color = "#d0d0d0"
    background_darkness = 0.78
    background_type = transparent
    font = Inconsolata Bold 12
    foreground_color = "#000000"
    show_titlebar = False
    use_system_font = False
